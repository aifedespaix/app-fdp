{
  "version": 2,
  "name": "fdp-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/node"
    }
  ],
  "engines": {
    "node": "10.x"
  },
  "routes": [
    {
      "src": "/(assets/.+|.+.css|.+.js)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.).html",
      "headers": {
        "cache-control": "public,max-age=0,must-revalidate"
      },
      "dest": "/$1.html"
    },
    {
      "src": "/(.)",
      "headers": {
        "cache-control": "public,max-age=0,must-revalidate"
      },
      "dest": "/index.html"
    }
  ]
}
